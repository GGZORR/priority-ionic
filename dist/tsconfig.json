{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": ".",
    "target": "es5",
    "lib": ["es6", "es2015", "dom"],
    "inlineSources": false,
    "stripInternal": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "../lib",
    "declaration": true
  },
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
     "genDir": "compiled",
     "entryModule": "/priority.module#PriorityIonicModule",
     "skipMetadataEmit" : true
  }
}